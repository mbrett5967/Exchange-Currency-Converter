<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             x:Class="CurrencyConvert.UI.App"
             RequestedThemeVariant="Default">
    <!-- "Default" ThemeVariant follows system theme variant. "Dark" or "Light" are other available options. -->

    <Application.Styles>
        <FluentTheme />
        <StyleInclude Source="avares://CurrencyConvert.UI/Styles/AppDefaultStyles.axaml"/>
    </Application.Styles>
    <Application.Resources>
        <SolidColorBrush x:Key="PrimaryBackground" Color="#111F36"/>
     <SolidColorBrush x:Key="BorderBackground" Color="#E4B754"/>          
            
    </Application.Resources>
</Application>